<?php

    use DynamicalWeb\DynamicalWeb;

    if(isset($_GET['auth']) && $_GET['auth'] == 'telegram')
    {
        $RequestHandler = DynamicalWeb::activeRequestHandler();
        $RequestHandler->Redirect = true;
        $RequestHandler->RedirectLocation = DynamicalWeb::getRoute('settings/login_security', $_GET);
        return;
    }