{
    "primary_language": "en",
    "router": [
        {"path": "/", "page": "index"},

        {"path": "/auth/login", "page": "login"},
        {"path": "/auth/register", "page": "register"},
        {"path": "/auth/verify", "page": "verify"},
        {"path": "/auth/sudo", "page": "sudo"},
        {"path": "/auth/verify/mobile", "page": "verify_mobile"},
        {"path": "/auth/verify/recovery_codes", "page": "verify_recovery_codes"},

        {"path": "/user/contents/public/avatar", "page": "avatar"},
        {"path": "/user/personal", "page": "personal"},

        {"path": "/security", "page": "login_security"},
        {"path": "/security/login_history", "page": "login_history"},
        {"path": "/security/setup/mobile", "page": "setup_mobile_verification"},
        {"path": "/security/setup/recovery_codes", "page": "setup_recovery_codes"}
    ],
    "libraries": {
        "SecuredWebSessions": {
            "directory_name": "sws",
            "autoloader": "sws.php",
            "check_class_exists": true,
            "namespace": "sws",
            "main_class": "sws"
        },
        "IntellivoidAccounts": {
            "directory_name": "IntellivoidAccounts",
            "autoloader": "IntellivoidAccounts.php",
            "check_class_exists": true,
            "namespace": "IntellivoidAccounts",
            "main_class": "IntellivoidAccounts"
        }
    },
    "runtime_scripts": {
        "initialize": [],
        "on_page_load": ["auth.check.php", "check.host.php"],
        "page_loaded": [],
        "after_request": [],
        "on_request": [],
        "on_import": []
    }
}
