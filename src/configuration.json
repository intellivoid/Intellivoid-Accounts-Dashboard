{
    "name": "Intellivoid Accounts",
    "version": "3.0.0.0",
    "author": "Zi Xing Narrakas",
    "organization": "Intellivoid Technologies",
    "configuration": {
        "root_path": "/",
        "localization": {
            "enabled": true,
            "primary_localization": "en",
            "auto_detect_preference": true,
            "localizations": {
                "en": "localization/en.json",
                "zh": "localization/zh.json",
                "zh-cn": "localization/zh.json",
                "tr": "localization/tr.json",
                "es": "localization/es.json"
            }
        },
        "sessions": {
            "enabled": true,
            "configuration_name": "Intellivoid Accounts"
        },
        "favicon": null,
        "localization_enabled": true,
        "debugging_mode": true,
        "framework_signature": true,
        "application_signature": true,
        "security_headers": true,
        "enable_khm": true,
        "firewall_deny": [],
        "headers": {}
    },
    "runtime_scripts": [
        {"event": "pre_page_load", "script": "runtime_scripts/auth.check.dyn"},
        {"event": "pre_page_load", "script": "runtime_scripts/check.host.dyn"}
    ],
    "web_assets": [
        {"type": "ppm", "name": "primary_assets", "source": "net.intellivoid.accounts_template==latest", "path": "assets_src"}
    ],
